<article class="listing">
  <img src="{{todoItem.image}}" alt="">
  <h3>{{todoItem.title}}</h3>
  <div class="detail owner">
    <span>Owner:</span> {{todoItem.owner}}
  </div>
  <div class="detail type">
    <span>Type:</span> {{todoItem.propertyType}}
  </div>
  <div class="detail location">
    <span>Location:</span> {{todoItem.city}}
  </div>
  <div class="detail bedrooms">
    <span>Number of bedrooms:</span> {{todoItem.bedrooms}}
  </div>
</article>